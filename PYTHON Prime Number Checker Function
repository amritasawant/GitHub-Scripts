    def is_prime(n):
        Prime=[1]
        NonPrime=[]
        if n>2:
            Prime.append(2)
        for i in range(3,n):
            if (i%2==0) or (i!=3 and i%3==0) or (i!=5 and i%5==0) or (i!=7 and i%7==0):
                NonPrime.append(i)
            else:
               Prime.append(i)
        print "Prime",Prime
        print "Non-Prime",NonPrime
